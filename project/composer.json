{
  "name": "jsdecena/laracom",
  "description": "Laravel powered e-commerce",
  "keywords": [
    "e-commerce",
    "ecommerce",
    "shopping-cart",
    "laravel",
    "laracom",
    "laravel-ecommerce",
    "shop",
    "laravel5",
    "framework",
    "package"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "barryvdh/laravel-dompdf": "^v2.0.1",
    "binarytorch/larecipe": "^v2.6.1",
    "doctrine/dbal": "^3.6.2",
    "fruitcake/laravel-cors": "^v3.0.0",
    "guzzlehttp/guzzle": "^7.7.0",
    "andresgarzonj/baserepo": "^v1.0.0",
    "jsdecena/mcpro": "1.1.0",
    "kalnoy/nestedset": "^v6.0.2",
    "laravel/framework": "^9.0",
    "laravel/legacy-factories": "^v1.3.2",
    "laravel/tinker": "^v2.8.1",
    "laravel/ui": "^v4.2.2",
    "nicolaslopezj/searchable": "^1.13.0",
    "paypal/rest-api-sdk-php": "1.14.0",
    "santigarcor/laratrust": "^7.2.0",
    "shippo/shippo-php": "^v2.0.0",
    "andresgarzonj/shoppingcart": "^1.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/andresgarzonj/LaravelShoppingcart"
    }
  ],
  "require-dev": {
    "fakerphp/faker": "^v1.22.0",
    "filp/whoops": "~2.15.2",
    "mockery/mockery": "~1.5.1",
    "phpunit/phpunit": "~10.1.3"
  },
  "autoload": {
    "files": [
      "app/Helpers/helper.php"
    ],
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform-check": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
